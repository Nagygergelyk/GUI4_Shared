<Window x:Class="GUI4_Shared.EditorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI4_Shared"
        mc:Ignorable="d"
        Title="EditorWindow" Height="500" Width="500">
    <Grid>
        <StackPanel>
            <Label Content="Név:"/>
            <TextBlock/>
            <Label Content="Ereje:" />
            <TextBlock/>
            <Label Content="Gyorsasága:" />
            <TextBlock/>
            <Label Content="Hova tartozása:" />
            <StackPanel Orientation="Horizontal">
                <RadioButton Content="Jó"/>
                <RadioButton Content="Rossz"/>
                <RadioButton Content="Semmleges" />
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
